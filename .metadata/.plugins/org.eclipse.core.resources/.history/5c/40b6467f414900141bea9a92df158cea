package test1;

public class Test1 {

	public static void main(String[] args) {
				int mas[][] = new int[24][24];
		
		  for (int y=0; y<24; y++)
		  {
			  for (int i=0; i<24; i++)
			  {
				  mas[y][i] = 1;
		     	System.out.print(""+mas[i][i]);
		     	if (i==23) 
		      { System.out.println();}
			  }
			  i=0;
		   }
	}

}
